{
    "name": "robotateme/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 5.",
    "abandoned": true,
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robotateme"
        }
    ],
  "require": {
      "php": ">=7.0",
      "barryvdh/laravel-ide-helper": "^v2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Robotateme\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0",
        "sebastian/environment": "^4.1",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Robotateme\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
